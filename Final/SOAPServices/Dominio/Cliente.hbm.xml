<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
  <class name="Cliente" table="cliente">
    <id name="Codigo" column="cod_cliente">
      <generator class="increment" />
    </id>
    <property name="RazonSocial" column="razon_social" />
    <property name="RUC" column="numero_documento" />
    <property name="Correo" column="correo" />
  </class>
</hibernate-mapping>